{
  "mcpServers": {
    "playwright": {
      "command": "bunx",
      "args": [
        "@playwright/mcp@latest",
        "--browser",
        "chromium",
        "--user-data-dir",
        "./.mcp-profile",
        "--headless",
        "--isolated",
        "--no-sandbox"
      ]
    },
    "figma": {
      "command": "bunx",
      "args": [
        "-y",
        "figma-developer-mcp",
        "--stdio"
      ]
    },
    "terraform": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "hashicorp/terraform-mcp-server"
      ]
    },
    "awslabs.aws-documentation-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.aws-documentation-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_DOCUMENTATION_PARTITION": "aws"
      }
    },
    "awslabs.aws-pricing-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.aws-pricing-mcp-server@latest"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "mcp-svelte-docs": {
      "command": "bunx",
      "args": [
        "-y",
        "mcp-svelte-docs"
      ]
    },
    "canva-dev": {
      "command": "bunx",
      "args": [
        "-y",
        "@canva/cli@latest",
        "mcp"
      ]
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${env:GH_TOKEN}"
      }
    }
  }
}